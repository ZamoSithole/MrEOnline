@model MrE.Models.Entities.Genre

@{
    ViewBag.Title = "Details";
}

<div class="card">
    <div class="card-header">@ViewBag.Title</div>
    <div class="card-body">
        @Html.Partial("_Details")
        <a href="@Url.Action("Edit", new { id = Model.Id })" class="btn btn-outline-primary">
            <i class="fa fa-save"></i>&nbsp;Edit
        </a>
        <a href="@Url.Action("index")" class="btn btn-outline-secondary">
            <i class="fa fa-chevron-circle-left"></i>&nbsp;Back
        </a>
    </div>
</div>