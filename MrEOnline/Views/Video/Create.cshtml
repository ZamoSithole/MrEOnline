@model MrE.Models.Entities.Video

@{
    ViewBag.Title = "Create Video";
}

<script src="~/Scripts/lodash.js"></script>
<div class="card">
    <div class="card-header">@ViewBag.Title</div>
    <div class="card-body">
        @using (Html.BeginForm("Create", "Video", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            @Html.AntiForgeryToken()
            @Html.Partial("_Form")
        }
    </div>
</div>