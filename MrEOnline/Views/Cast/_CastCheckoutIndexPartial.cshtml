@model IEnumerable<MrE.Models.Entities.Cast>

<div class="panel panel-default">
    @*<div class="panel-heading">@ViewBag.Title</div>*@
    <div class="panel-body">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Title.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Video.Title)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FirstName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.LastName)
                </th>

                <th></th>
            </tr>

            @foreach (var item in Model) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Title.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Video.Title)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirstName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LastName)
                    </td>
                </tr>
            }

        </table>
    </div>
</div>